    "build:apk:staging": "node scripts/copy-company-assets.js && cd android && ./gradlew assembleStagingRelease",
    "build:apk:prod": "node scripts/copy-company-assets.js && cd android && ./gradlew assembleProductionRelease",
    "build:apk:debug": "node scripts/copy-company-assets.js && cd android && ./gradlew assembleDebug",
    "build:aab": "node scripts/copy-company-assets.js && cd android && ./gradlew bundleRelease && cd ..",
    "copy:assets": "node scripts/copy-company-assets.js",
    "generate:loaders": "node scripts/generate-plugin-loaders.js",
    "split:company": "node scripts/split-company-repo.js",
    "split:all": "node scripts/split-company-repo.js --all",
